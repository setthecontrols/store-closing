<% form_for (@user) do |f|%>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <%= f.label :user_id %>
  <%= f.text_field :user_id %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit   %>
<% end %>
